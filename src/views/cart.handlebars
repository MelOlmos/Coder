<div>
  <h1>Carrito solo</h1>
  {{#each cart}}
    {{#each this.products}}
      {{log this}}
      <ul>
        <li> <strong>Title:</strong> {{this.title}}</li>
        <li> <strong>Description:</strong> {{this.description}}</li>
        <li> <strong>Category:</strong> {{this.category}}</li>
        <li> <strong>Precio:</strong> {{this.price}}</li>
        <li> <strong>Cantidad:</strong> {{this.quantity}}</li>        
      </ul>
    {{/each}}
  {{/each}}
</div>